QVBoxLayout {
	margin: 1.5em auto 1.5em auto;
  	border: .1em;
  	padding: .9em;
}

QLabel { 
	background-color: white;
    border-style: outset;
    border-width: 2px;
    border-radius: 10px;
    border-color: white;
    font: bold 14px;
    min-width: 100em;
    padding: 6px;
}


QScrollBar:vertical {
     border-style: outset;
     border-width: 2px;
     border-radius: 5px;
     border-color: transparent;
     background: #486b8a;
     width: 15px;
     height: 200px;
     margin: 22px 0 22px 0;
 }

 QScrollBar::handle:vertical {
     background: white;
     height: 10px;
     border-style: outset;
     border-width: 2px;
     border-radius: 5px;
     border-color: transparent;

 }

 QScrollBar::add-line:vertical {
     border: 2px solid grey;
     background: #486b8a;
     height: 20px;
     border-style: outset;
     border-width: 2px;
     border-radius: 5px;
     border-color: transparent;
     subcontrol-position: bottom;
     subcontrol-origin: margin;
 }

 QScrollBar::sub-line:vertical {
     border: 2px solid grey;
     background: #486b8a;
     height: 20px;
     border-style: outset;
     border-width: 2px;
     border-radius: 5px;
     border-color: transparent;
     subcontrol-position: top;
     subcontrol-origin: margin;
 }

 QScrollBar::up-arrow:vertical, QScrollBar::down-arrow:vertical {
     border: 2px solid grey;
     width: 3px;
     height: 1px;
     background: white;
 }

 QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
     background: none;
 }