QPushButton[accessibleName="Red"] {
    background-image: url("Red.png");
    border-style: outset;
    border-width: 2px;
    border-radius: 5px;
    border-color: transparent;
    font: bold 14px;
    min-width: 10em;
    padding: 0;
    margin: 0;
    width: 190px;
    height: 28px;
}


QPushButton[accessibleName="Red"]:pressed {
    background-image: url("RedPressed.png");
    border-style: outset;
}

